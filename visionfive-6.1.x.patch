+
+static int ytphy_set_wol(struct phy_device *phydev, struct ethtool_wolinfo *wol)
+{
+	struct ytphy_wol_cfg wol_cfg = {};
+	struct net_device *p_attached_dev = phydev->attached_dev;
+	int ret, pre_page, val;
