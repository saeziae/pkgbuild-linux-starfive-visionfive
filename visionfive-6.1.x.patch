+	pre_page = ytphy_read_ext(phydev, YT8521_EXT_SMI_SDS_PHY);
+	if (pre_page < 0)
+		return pre_page;
+
+	/* switch to UTP access */
+	ret = ytphy_write_ext(phydev, YT8521_EXT_SMI_SDS_PHY, YT8521_SMI_SDS_PHY_UTP);
+	if (ret < 0)
