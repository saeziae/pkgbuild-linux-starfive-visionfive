+		/* Set the WOL config */
+		wol_cfg.enable = 1; //enable
+		wol_cfg.type = YTPHY_WOL_TYPE_PULSE;
+		wol_cfg.width = YTPHY_WOL_WIDTH_672MS;
+		ret = ytphy_wol_en_cfg(phydev, &wol_cfg);
+		if (ret < 0)
